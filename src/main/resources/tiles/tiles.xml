    <definition name="*/*/*/*/*/*/*.lnb" extends=".template-lnb">
        <put-attribute name="content" value="/WEB-INF/jsp/chungnam/portal/{1}/{2}/{3}/{4}/{5}/{6}/{7}.jsp" />
    </definition>
    <definition name="*/*/*/*/*/*/*/*.lnb" extends=".template-lnb">
