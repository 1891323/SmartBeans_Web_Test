<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">

<tiles-definitions>

    <!-- extends 공통으로 사용할 타일즈 정의 -->
    <definition name=".extends-commons">
        <put-attribute name="commons" value="/WEB-INF/jsp/chungnam/portal/layouts/tiles/extends/LayoutGlobals.jsp" />
    </definition>

    <!-- INDEX(메인) 페이지 -->
    <!-- INDEX PAGE -->
    <definition name=".template-index" template="/WEB-INF/jsp/chungnam/portal/layouts/tiles/template/LayoutIndex.jsp" extends=".extends-commons">
        <put-attribute name="header" 			value="/WEB-INF/jsp/chungnam/portal/layouts/tiles/attribute/com/LayoutHeader.jsp"/>
        <put-attribute name="footer" 			value="/WEB-INF/jsp/chungnam/portal/layouts/tiles/attribute/com/LayoutFooter.jsp"/>
        <put-attribute name="content" value="" />
    </definition>

    <!-- SUB PAGE -->
    <definition name=".template-lnb" template="/WEB-INF/jsp/chungnam/portal/layouts/tiles/template/LayoutLnb.jsp" extends=".extends-commons">
        <put-attribute name="header" 			value="/WEB-INF/jsp/chungnam/portal/layouts/tiles/attribute/com/LayoutHeader.jsp"/>
        <put-attribute name="footer" 			value="/WEB-INF/jsp/chungnam/portal/layouts/tiles/attribute/com/LayoutFooter.jsp"/>
        <put-attribute name="left" 			value="/WEB-INF/jsp/chungnam/portal/layouts/tiles/attribute/com/LayoutLeft.jsp"/>
        <put-attribute name="content" value="" />
    </definition>

    <!-- ACTION -->
    <!-- LEFT NAVIGATION -->
    <definition name="*.lnb" extends=".template-lnb">
        <put-attribute name="content" value="/WEB-INF/jsp/chungnam/portal/{1}.jsp" />
    </definition>
    <definition name="*/*.lnb" extends=".template-lnb">
        <put-attribute name="content" value="/WEB-INF/jsp/chungnam/portal/{1}/{2}.jsp" />
    </definition>
    <definition name="*/*/*.lnb" extends=".template-lnb">
        <put-attribute name="content" value="/WEB-INF/jsp/chungnam/portal/{1}/{2}/{3}.jsp" />
    </definition>
    <definition name="*/*/*/*.lnb" extends=".template-lnb">
        <put-attribute name="content" value="/WEB-INF/jsp/chungnam/portal/{1}/{2}/{3}/{4}.jsp" />
    </definition>
    <definition name="*/*/*/*/*.lnb" extends=".template-lnb">
        <put-attribute name="content" value="/WEB-INF/jsp/chungnam/portal/{1}/{2}/{3}/{4}/{5}.jsp" />
    </definition>
    <definition name="*/*/*/*/*/*.lnb" extends=".template-lnb">
        <put-attribute name="content" value="/WEB-INF/jsp/chungnam/portal/{1}/{2}/{3}/{4}/{5}/{6}.jsp" />
